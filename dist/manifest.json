{
  "name": "Cinema 8",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "tabCapture",
    "downloads",
    "storage",
    "identity",
    "downloads.shelf",
    "desktopCapture",
    "scripting",
    "declarativeNetRequest"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./obj-16x16.png",
    "32": "./obj-32x32.png",
    "48": "./obj-48x48.png",
    "128": "./obj-128x128.png"
  },
  "description": "Screen Recorder",
  "options_page": "./options.html"
}
