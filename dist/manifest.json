{
  "name": "Cinema 8",
  "description": "Screen recorder",
  "version": "0.1.1",
  "manifest_version": 3,
  "options_page": "options.html",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "tabCapture",
    "downloads",
    "storage",
    "identity",
    "downloads.shelf",
    "desktopCapture",
    "scripting",
    "declarativeNetRequest"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.index.jsx.6bd9233b.f6e1e6a6.js",
        "assets/content-script-loader.index.jsx.e0431b2d.765d04c1.js",
        "assets/content-script-loader.ffmpeg-core.js.ec57f814.c0e91136.js",
        "assets/content-script-loader.ffmpeg.min.js.f50bac52.acaaa3cf.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "assets/index.88f1f3ee.css",
        "assets/index.0622ebaa.css"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "vendor/*"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/functions.c65ca704.js",
        "assets/index.esm.f158f7be.js",
        "assets/index.f4663c05.js",
        "assets/index.jsx.6bd9233b.js",
        "assets/index.c1ab3068.js",
        "assets/index.jsx.e0431b2d.js",
        "assets/ffmpeg-core.js.ec57f814.js",
        "assets/ffmpeg.min.js.f50bac52.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval' 'wasm-unsafe-eval'; child-src 'self';"
  }
}